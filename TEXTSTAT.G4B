if /i "%COUNT:~-8,8%"=="\x0D\x0A" && if %countlen%<=30 && set eol=\x0D\x0A && set countA=%count:~0,16% && set countB=%count:~16,14% &&  && set /a countlen=%countlen%+2 && goto :countdone ! if /i "%COUNT:~-8,8%"=="\x0D\x0A" && if not %countlen%<=30 && set countA=%count:~0,16% && set countB=%count:~16,16% && set countC=%count:~32,14% && set /a countlen=%countlen%+2 && goto :countdone
