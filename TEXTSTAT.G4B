if /i "%COUNT:~-4,4%"=="\x0D" && if %countlen%<=31 && set eol=\x0D && set countA=%count:~0,16% && set countB=%count:~16,15% && set /a countlen=%countlen%+1 && goto :countdone ! if /i "%COUNT:~-4,4%"=="\x0D" && if not %countlen%<=31 && set eol=\x0D && set countA=%count:~0,16% && set countB=%count:~16,16% && set countC=%count:~32,15% && set /a countlen=%countlen%+1 && goto :countdone
set countA=%count:~0,16% && set countB=%count:~16,16% && set countC=%count:~32,16%
:countdone
